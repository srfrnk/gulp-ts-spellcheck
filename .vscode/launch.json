    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/src/debug.ts",
            "outFiles": [
                "${workspaceFolder}/build/**/*",
            ],
            "sourceMaps": true,
            "preLaunchTask": "gulp: tsc"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp jest",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "jest"
            ],
            "outFiles": [
                "${workspaceFolder}/build/**/*",
            ],
            "sourceMaps": true,
            "preLaunchTask": "gulp: tsc"
       },
    ]
}