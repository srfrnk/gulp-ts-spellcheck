{
  "name": "gulp-ts-spellcheck",
  "version": "1.1.4",
  "description": "spellcheck typescript code with gulp",
  "main": "index.js",
  "engines": {
    "node": "^8"
  },
  "scripts": {
    "postinstall-dev": "cp ./.githooks/* .git/hooks",
    "postinstall-production": "",
    "postinstall": "if-env NODE_ENV=production && npm run postinstall-production || npm run postinstall-dev",
    "tslint": "gulp tslint",
    "tsc": "gulp tsc",
    "jest": "gulp jest",
    "tsc-jest": "npm run tsc jest",
    "release": "standard-version",
    "publish-release": "(git push --follow-tags origin master) && (npm publish)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srfrnk/gulp-ts-spellcheck.git"
  },
  "keywords": [
    "gulp",
    "typescript",
    "spell",
    "check"
  ],
  "author": "srfrnk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/srfrnk/gulp-ts-spellcheck/issues"
  },
  "homepage": "https://github.com/srfrnk/gulp-ts-spellcheck#readme",
  "devDependencies": {
    "@types/gulp-util": "^3.0.34",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.7",
    "@types/vinyl": "^2.0.2",
    "gulp": "^4.0.0",
    "gulp-dest": "^0.2.3",
    "gulp-jest": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "from2": "^2.3.0",
    "get-line-from-pos": "^1.0.0",
    "gulp-util": "^3.0.8",
    "if-env": "^1.0.4",
    "spellchecker": "^3.4.4",
    "through2": "^2.0.3",
    "typescript-parser": "^2.5.0",
    "vinyl": "^2.2.0"
  }
}
